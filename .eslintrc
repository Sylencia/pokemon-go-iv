{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "indent": [2],
    "no-param-reassign": [1, { "props": false }],
    "react/jsx-closing-bracket-location": [1, 'after-props'],
    "new-cap": [0],
    "react/prefer-stateless-function": 0,
    "no-multiple-empty-lines": 0,
    # "import/no-unresolved": [2, { ignore: ['\~*'] }],
  }
}
